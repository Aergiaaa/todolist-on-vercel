{
   "version": 2,
   "builds": [
      {
         "src": "api/*.go",
         "use": "@vercel/go"
      }
   ],
   "routes": [
      {
         "src": "/static/(.*)",
         "dest": "/static/$1"
      },
      {
         "src": "/templates/(.*)",
         "dest": "/templates/$1"
      },
      {
         "src": "/(.*)",
         "dest": "/api/$1"
      }
   ]
}